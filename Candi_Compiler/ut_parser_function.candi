#func foo {}; // Shorthand Void Arg Method Definition
#func foo (a, b, c) {}; // Unconstrained Method Definition
#func foo() {};
#func [&int] foo {}; // Constrained Shorthand Void Arg Method Definition
#func [&int] foo (a, b, c) {}; // Constrained Method Definition

// Function definition with return
#func foo {
	#return 1;
};

// Function definition with return and multiple statements
#func foo {
	a = 1;
	b = 2;
	#return a + b;
};